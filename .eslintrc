{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle":["off","never"],
    "indent": ["off", 2, {"SwitchCase": 1}],
    "linebreak-style": ["off", "windows"],
    "quotes": ["off", "single"],
    "semi": ["off", "always"],
    "strict": [2, "global"]
  }
}